{
  "name": "svelte-app",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "rollup -c --environment production",
    "dev": "concurrently --names \"Django,HTTPServer,Svelte\" -c \"bgBlue.bold,bgGreen.bold,bgYellow.bold\" \"npm run dev-backend\" \"npm run dev-frontend\" \"npm run dev-svelte\"",
    "dev-backend": "python3 manage.py runserver",
    "dev-frontend": "cd static && python3 serve.py",
    "dev-svelte": "rollup -c -w"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^24.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-terser": "^0.4.0",
    "concurrently": "^9.2.0",
    "rollup": "^3.15.0",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-css-only": "^4.3.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.1.2",
    "svelte-preprocess": "^6.0.3"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "bootstrap": "^5.3.3",
    "font-awesome": "^4.7.0",
    "sirv-cli": "^2.0.0",
    "svelte": "^4.2.18",
    "svelte-app": "file:",
    "svelte-routing": "^2.13.0"
  }
}
